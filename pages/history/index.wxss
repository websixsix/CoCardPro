/* pages/history/index.wxss */
.calendar {
  --calendar-height: 650px;
  transition: all .3s;
}
.pull-down{
  transition: all .3s;
  --calendar-height: 200rpx;
  position: fixed;
  top: 0;
  left: 0;
}
.content{
  width: 100%;
  height: 100vh;
  padding-bottom: 15rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.int-box{
  width: 100%;
  height: auto;
  text-align: center;
}
