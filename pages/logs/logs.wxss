.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
.def-box,.col-box{
  width: 95%;
  height: 300rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin: 0 auto;
  background-color: #fff;
  border-radius: 15rpx;
  margin-top: 30rpx;
}
.def-sty{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  background-color: yellow;
}
.col-box{
  height: 200rpx;
  flex-direction: column;
  justify-content: center;
  margin: 30rpx auto;
}
.left-box,.right-box{
  width: 45%;
  height: 100%;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-radius: 15rpx;
}
.btn-view{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 30rpx auto;
}

.over-init{
  margin-left:30rpx;
  text-overflow: ellipsis;
  display: inline-block;
  height: auto;
  width: 400rpx;
}